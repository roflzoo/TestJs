// Пузырёк

function bubbleSort(arr) {
    let n = arr.length;
    for (let i = 0; i < n; i++) {
        let swapped = false;
        for (let j = 0; j < n - i - 1; j++) {
            if (arr[j] > arr[j + 1]) {
                [arr[j], arr[j + 1]] = [arr[j + 1], arr[j]];
                swapped = true;
            }
        }
        if (!swapped) {
            break;
        }
    }
    return arr;
}

const array = [64, 34, 25, 12, 22, 11, 90];
const sortedArray = bubbleSort(array);
console.log("Отсортированный массив:", sortedArray);
function runTests() {
    let result = bubbleSort([64, 34, 25, 12, 22, 11, 90]);
    console.assert(JSON.stringify(result) === JSON.stringify([11, 12, 22, 25, 34, 64, 90]), 'Тест 1 не пройден');
 const startTime = performance.now();
        bubbleSort([64, 34, 25, 12, 22, 11, 90]);
const endTime = performance.now();
    console.assert((endTime- startTime) < 0.5, 'Тест 2 не пройден');
}
runTests();
